{
  "name": "@near/soft-enclave-worker",
  "version": "0.1.0",
  "description": "Web Worker backend for soft-enclave",
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/browser.global.js",
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "serve": "node ../../enclave-server.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    }
  },
  "dependencies": {
    "@near/soft-enclave-core": "workspace:*",
    "@near/soft-enclave-shared": "workspace:*",
    "quickjs-emscripten": "*"
  },
  "devDependencies": {
    "rimraf": "*",
    "tsup": "*",
    "typescript": "*"
  },
  "keywords": [
    "soft-enclave",
    "worker",
    "backend"
  ],
  "author": "",
  "license": "MIT"
}
