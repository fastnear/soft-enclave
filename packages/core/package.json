  "dependencies": {
    "@near/soft-enclave-shared": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "*",
    "tsup": "*",
    "typescript": "*"
  },
  "keywords": [
    "soft-enclave",
    "core"
  ],
  "author": "",
  "license": "MIT"
}
