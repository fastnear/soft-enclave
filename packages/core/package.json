{
  "name": "@near/soft-enclave-core",
  "version": "0.1.0",
  "description": "Core enclave factory and base classes",
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
